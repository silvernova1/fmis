<div id="sidebar" class="sidebar                  responsive                    ace-save-state">
    <script type="text/javascript">
            try { ace.settings.loadState('sidebar') } catch (e) { }
    </script>

    <div class="sidebar-shortcuts" id="sidebar-shortcuts">
        <div class="sidebar-shortcuts-large" id="sidebar-shortcuts-large">
            <button class="btn btn-success">
                <i class="ace-icon fa fa-signal"></i>
            </button>

            <button class="btn btn-info">
                <i class="ace-icon fa fa-pencil"></i>
            </button>

            <button class="btn btn-warning">
                <i class="ace-icon fa fa-users"></i>
            </button>

            <button class="btn btn-danger">
                <i class="ace-icon fa fa-cogs"></i>
            </button>
        </div>

        <div class="sidebar-shortcuts-mini" id="sidebar-shortcuts-mini">
            <span class="btn btn-success"></span>

            <span class="btn btn-info"></span>

            <span class="btn btn-warning"></span>

            <span class="btn btn-danger"></span>
        </div>
    </div><!-- /.sidebar-shortcuts -->

    <ul class="nav nav-list">
        <li class="
            @if (@ViewBag.filter_sidebar == "dashboard")
                @Html.Raw("active")
            ">
            <a asp-area="" asp-controller="Home" asp-action="Index">
                <i class="menu-icon fa fa-tachometer"></i>
                <span class="menu-text"> Dashboard</span>
            </a>
        </li>

        <li class="">
            <a href="#" class="dropdown-toggle">
                <i class="menu-icon fa fa-desktop"></i>
                <span class="menu-text">
                    Budget
                </span>

                <b class="arrow fa fa-angle-down"></b>
            </a>

            <b class="arrow"></b>

            <ul class="submenu">
                <li class="">

                    <a asp-controller="Prexcs" asp-action="Index">


                        <i class="menu-icon fa fa-caret-right"></i>
                        PREXC
                    </a>
                </li>
            </ul>
        </li>

        <!-- UPLOAD FILE -->
        <li class="@if(ViewBag.filter_sidebar == "upload_file/personal_information")
                @Html.Raw("active open")
        ">
            <a href="#" class="dropdown-toggle">
                <i class="menu-icon fa fa-upload"></i>
                <span class="menu-text">
                    Upload Data
                </span>

                <b class="arrow fa fa-angle-down"></b>
            </a>

            <b class="arrow"></b>

            <ul class="submenu">
                <li class="
                    @if(ViewBag.filter_sidebar == "upload_file/personal_information")
                        @Html.Raw("active")
                ">
                    <a asp-controller="UploadFile" asp-action="UploadFile">
                        <i class="menu-icon fa fa-caret-right"></i>
                        Personal Information
                    </a>
                </li>

                <li class="">
                    <a href="typography.html">
                        <i class="menu-icon fa fa-caret-right"></i>
                        Division
                    </a>
                </li>

                <li class="">
                    <a href="elements.html">
                        <i class="menu-icon fa fa-caret-right"></i>
                        Section
                    </a>

                </li>

                <li class="">
                    <a href="buttons.html">
                        <i class="menu-icon fa fa-caret-right"></i>
                        ORS
                    </a>
                </li>
            </ul>
        </li>

        <li class="">
            <a asp-area="" asp-controller="UploadFile" asp-action="UploadFile">
                <i class="menu-icon fa fa-list-alt"></i>
                <span class="menu-text"> Upload File </span>
            </a>
        </li>

        <li class="@if(ViewBag.filter_sidebar == "gallery")
                        @Html.Raw("active")">
            <a asp-controller="Home" asp-action="Gallery">
                <i class="menu-icon fa fa-picture-o"></i>
                <span class="menu-text"> Gallery </span>
            </a>

            <b class="arrow"></b>
        </li>

    </ul><!-- /.nav-list -->

    <div class="sidebar-toggle sidebar-collapse" id="sidebar-collapse">
        <i id="sidebar-toggle-icon" class="ace-icon fa fa-angle-double-left ace-save-state" data-icon1="ace-icon fa fa-angle-double-left" data-icon2="ace-icon fa fa-angle-double-right"></i>
    </div>
</div>
