@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Procurement" + " " + "(AgencyToAgency)";
}

<style>

    .card-container {
        width: calc(70% - 10px); /* Adjust the total width by considering padding (20px on each side) */
        padding: 20px;
        border: 1px solid #ccc;
        border-radius: 5px;
        box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
        background-color: #fff;
        margin-left: 50px;
    }

    /* Media query for tablets (landscape and portrait) */
    @@media (max-width: 1024px) {
        .card-container {
            width: calc(80% - 40px); /* Adjust width for tablets */
        }
    }

    /* Media query for mobile devices */
    @@media (max-width: 767px) {
        .background-wrapper {
            min-height: 100vh;
        }

        .card-container {
            width: 90%; /* Adjust width for mobile devices */
        }
    }



    .rectangle h2 {
        white-space: nowrap;
        margin-top: 5px; /* Add space at the top */
        margin-bottom: 5px; /* Add space at the bottom */
    }

    .rectangle h6 {
        white-space: nowrap;
    }

    .rectangle {
        border: 1px solid black;
        padding-left: 100px;
        padding-right: 100px;
        margin-top: 10px;
        margin-bottom: 10px;
        width: fit-content;
        display: inline-block;
    }

    .TimesNewRoman {
        font-family:'Times New Roman';
        margin: 0;
        padding: 0;
    }



    .grid-container {
        display: grid;
        grid-template-columns: repeat(3, 1fr); /* 3 columns with equal width */
       
    }

    .grid-item {
        padding: 5px; /* Add padding for spacing within each grid item */
        text-align: center;
        margin-top: 20px; /* A/* Center-align the text within each grid item */
    }

    .logo-container {
        float: left; /* Float the logo container to the left */
        /* Adjust the margin as needed */
    }

    /* Style for the logo image */
    .logo-image {
        max-width: 100px; /* Adjust the max-width of the logo image */
    }



</style>
