@model IEnumerable<fmis.Models.Requesting_office>

@{
    ViewData["Title"] = "Index";
}

<h2>List of ORS Head Requesting Office /Authorized Representative</h2>

<p>
    <a class="btn btn-success" asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Head Name
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Position)
            </th>
            <th> Actions</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Head_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Position)
            </td>
            <td>
                <a class="btn btn-primary"asp-action="Edit" asp-route-id="@item.Id">Update</a> |
                @*<a asp-action="Details" asp-route-id="@item.Id">Details</a> |*@
                <a class="btn btn-danger"asp-action="Delete" asp-route-id="@item.Id">Cancel</a>
            </td>
        </tr>
}
    </tbody>
</table>
