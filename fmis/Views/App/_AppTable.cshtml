@model List<fmis.Models.ppmp.AppModel>

<h2>App List</h2>

<table class="table" id="appTable">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var app in Model)
        {
            <tr>
                <td>@app.Uacs</td>
                <td>@app.ProcurementProject</td>
                <td>
                    <a class="edit-link" data-id="@app.Id" href="#">Edit</a> |
                    <a class="delete-link" data-id="@app.Id" href="#">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<button id="addButton" class="btn btn-primary">Add Row</button>